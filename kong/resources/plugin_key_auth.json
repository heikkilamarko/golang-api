{
  "id": "${KEY_AUTH_PLUGIN_ID}",
  "name": "key-auth",
  "service": {
    "id": "${SERVICE_ID}"
  },
  "config": {
    "key_in_query": false,
    "key_names": ["x-api-key"],
    "key_in_header": true,
    "run_on_preflight": true,
    "hide_credentials": false,
    "key_in_body": false
  },
  "protocols": ["http"]
}
